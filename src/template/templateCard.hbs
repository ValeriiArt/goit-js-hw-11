{{#each this}}
    <div class="photo-card">
        <a href="{{largeImageURL}}">
             <img class="photo-card__img" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" height="240" />
        </a>
        <div class="info">
            <p class="info-item">Likes
                <b>{{likes}}</b>
            </p>
            <p class="info-item">Views
                <b>{{views}}</b>
            </p>
            <p class="info-item">Comments
                <b>{{comments}}</b>
            </p>
            <p class="info-item">Downloads
                <b>{{downloads}}</b>
            </p>
        </div>
    </div>
{{/each}}